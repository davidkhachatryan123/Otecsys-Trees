  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (TreeSQL)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "cwd": "/app/TreeSQL/bin/Debug/net8.0",
      "program": "TreeSQL.dll",
      "pipeTransport": {
        "pipeCwd": "${workspaceRoot}",
        "pipeProgram": "docker",
        "pipeArgs": [
          "exec",
          "-i",
          "tree-sql",
          "/bin/sh",
          "-c"
        ],
        "debuggerPath": "/remote_debugger/vsdbg"
      }
    },
    {
      "name": ".NET Core Launch (TreeElasticStack)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "cwd": "/app/TreeElasticStack/bin/Debug/net8.0",
      "program": "TreeElasticStack.dll",
      "pipeTransport": {
        "pipeCwd": "${workspaceRoot}",
        "pipeProgram": "docker",
        "pipeArgs": [
          "exec",
          "-i",
          "tree-elasticstack",
          "/bin/sh",
          "-c"
        ],
        "debuggerPath": "/remote_debugger/vsdbg"
      }
    }
  ],
  "compounds": [
    {
      "name": "Trees Otecsys",
      "configurations": [
        ".NET Core Launch (TreeSQL)",
        ".NET Core Launch (TreeElasticStack)",
      ],
      "preLaunchTask": "docker-compose: up",
      "stopAll": true
    }
  ]
}